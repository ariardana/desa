    "@types/morgan": "^1.9.9",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.3",
    "typescript": "^5.5.3",
    "express": "^4.18.3",
    "cors": "^2.8.5",
